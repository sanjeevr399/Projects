@model IEnumerable<MVC_Solution_1.Models.Customer>
@{
    ViewBag.Title = "AllCustomers";
}

<h2>AllCustomers</h2>

<table class="table">
    <tr>
        <th>Customer ID</th>
        <th>Company Name</th>
        <th>Contact Name</th>
    </tr>
    @foreach (var customer in Model)
    {
        <tr>
            <td>@customer.CustomerID</td>
            <td>@customer.CompanyName</td>
            <td>@customer.ContactName</td>
        </tr>
    }
</table>